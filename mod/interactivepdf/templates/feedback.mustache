<link rel="stylesheet" href="../mod_interactivepdf_style.css">

<div class="w-100">
    {{{content}}}
</div>

<div id="final_feedback_submit" class="d-none container-fluid tertiary-navigation">
    <div class="row">
        <div class="singlebutton quizstartbuttondiv">
            <form method="post" action="{{{formurl}}}">
                <input type="hidden" name="id" value="{{cmid}}">
                <input type="hidden" name="attemptid" value="{{attemptid}}">
                <input type="hidden" name="pageid" value="{{pageid}}">
                <input type="hidden" name="studentid" value="{{studentid}}">
                <button type="submit" class="btn btn-primary" id="single_button_submit_feedback">Submit Feedback</button>
            </form>
        </div>
    </div>
</div>